<% if current_user.has_role? :student %>
    <%
        if !current_user.student_fixed_info
            current_user.student_fixed_info = StudentFixedInfo.new
            current_user.student_fixed_info.user_id = current_user.id
            current_user.student_fixed_info.save
        end
    %>
    <%= render :partial => "/home/profile_student",locals: {:user => current_user} %>
<%elsif current_user.has_role? :lecturer %>
    <%= render :partial => "/home/profile_lecturer",locals: {:user => current_user} %>
<% elsif current_user.has_role? :partner %>
    <%= render :partial => "/home/profile_partner",locals: {:user => current_user} %>

<%elsif current_user.has_role? :admin %>

<% end %>