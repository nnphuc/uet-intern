<div class="col-sm-10 col-sm-offset-1">
<%= form_for @info,url: {:action => "update" } do |f| %>

   <div class="form-group row">
      <%=f.label :emailcanhan,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.email_field :emailcanhan,type: "email", class:"form-control",placeholder: "Enter email" %>
      </div>
    </div>

    <div class="form-group row">
      <%=f.label :skypeid,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.text_field :skypeid,type: "text", class:"form-control",placeholder: "Enter skype id" %>
      </div>
    </div>

    <div class="form-group row">
      <%=f.label :facebook,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.text_field :facebook,type: "text", class:"form-control",placeholder: "Enter facebook" %>
      </div>
    </div>

    <div class="form-group row">
      <%=f.label :dienthoai,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.text_field :dienthoai,type: "text", class:"form-control",placeholder: "Enter phone number" %>
      </div>
    </div>

     <div class="form-group row">
      <%=f.label :vitricanbo,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.text_field :vitricanbo,type: "text", class:"form-control",placeholder: "vi tri can bo" %>
      </div>
    </div>
     <div class="form-group row">
      <%=f.label :ngoaingu,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.text_field :ngoaingu,type: "text", class:"form-control",placeholder: "ngoai ngu" %>
      </div>
    </div>
     <div class="form-group row">
      <%=f.label :chungchi,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.text_field :chungchi,type: "text", class:"form-control",placeholder: "chung chi" %>
      </div>
    </div>
     <div class="form-group row">
      <%=f.label :kinhnghiem,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.text_field :kinhnghiem,type: "text", class:"form-control",placeholder: "Kinh nghiem" %>
      </div>
    </div>
     <div class="form-group row">
      <%=f.label :mongmuon,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.text_field :mongmuon,type: "text", class:"form-control",placeholder: "Mong muon" %>
      </div>
    </div>
     <div class="form-group row">
      <%=f.label :ghichu,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.text_field :ghichu,type: "text", class:"form-control",placeholder: "ghi chu" %>
      </div>
    </div>

    <div class="form-group row">
      <%=f.label :avatar,class:"col-form-label col-sm-2"%>
      <div class="col-sm-10">

        <%= f.file_field :avatar %>
      </div>
    </div>
    <div class="form-group row">

      <div class="col-sm-10 offset-2">

        <%= f.button :submit,class:"btn btn-outline-primary" %>

      </div>
    </div>
<%end%>
</div>
